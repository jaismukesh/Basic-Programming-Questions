#include <iostream>
using namespace std;
int fibonaci(int n){
    if(n==0){
        return 0;
    }
    else if(n==1){
        return 1;
    }
    else{
        return fibonaci(n-1)+fibonaci(n-2);
    }
}
int main()
{
    int n;
    cout<<"enter the value:"<<endl;
    cin>>n;
    cout<<"fibonaci series is:"<<endl;
   for(int i=0;i<=n;i++){
       cout<<fibonaci(i)<<" ";
   }
}
